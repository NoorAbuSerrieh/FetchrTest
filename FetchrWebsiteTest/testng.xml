<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="FetchrWebsite">


	<test thread-count="5" name="End to End Test on chrome">
		<parameter name="browser" value="chrome"></parameter>
		<classes>
			<class name="com.fetchr.testcases.TrackingPageTest"></class>
			<class name="com.fetchr.testcases.CoronaSafetyPageTest" />
			<class name="com.fetchr.testcases.PrivacyPolicyPageTest" />
			<class name="com.fetchr.testcases.TermsAndConditionsPageTest" />
			<class name="com.fetchr.testcases.config"></class>
		</classes><!-- -->
	</test> <!-- End to End Test -->


	<test thread-count="5" name="End to End Test on firefox">
		<parameter name="browser" value="firefox"></parameter>
		<classes>
			<class name="com.fetchr.testcases.TrackingPageTest" />
			<class name="com.fetchr.testcases.CoronaSafetyPageTest" />
			<class name="com.fetchr.testcases.PrivacyPolicyPageTest" />
			<class name="com.fetchr.testcases.TermsAndConditionsPageTest" />
			<class name="com.fetchr.testcases.config"></class>
		</classes><!-- -->
	</test> <!-- End to End Test -->




</suite> <!-- FetchrWebsite -->
